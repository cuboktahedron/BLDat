{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es5",
      "es2015.iterable",
      "scripthost"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "baseUrl": "./src",
    "paths": {
      "ts": [ "./ts/*", "../test/ts/*" ]
    },
    "target": "es5",
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}